<%-include('header')%>
<section id = "community-area">
  <div class="container">
    <div class="row align-items-start">
  <%usrContent.forEach(ele=>{%>
        <div class="col-md-4">
          <div class="card mb-5" style="width: 18rem;">
            <img src="<%=ele.imageUrl %>" class="card-img-top posts" alt="...">
            <div class="card-body">
              <h5 class="card-title"><%=ele.title%></h5>
              <p class="card-text comment"><%=ele.comment%></p>
              <a href="#" class="btn btn-outline-danger">Read More</a>
            </div>
          </div>
        </div>
  <%})%>
  </div>
  </div>
</section>


<%-include('footer')%>
